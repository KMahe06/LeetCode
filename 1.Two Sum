class Solution {
    public int[] twoSum(int arr[],int c) {


     int res[]=new int[2];
     for(int i=0;i<arr.length;i++){
          for(int j=0;j<arr.length;j++){
              if(arr[i]+arr[j]==c && i!=j){
                  res[0]=i;
                  res[1]=j;
                  break;
              }
          }
     }
     return res;
     
    }}
